<body id="tutorBody">

  <!-- jumbotron -->
  <div class="jumbotron jumbotron-fluid">
    <div class="container">
      <div class="row">
        <div class="row">
          <div class="col-xs-12" align="left">
            <div class="col-xs-3">
              <form id='uploadImage' action='/uploadProfileImage' method='POST' encType="multipart/form-data">
                <label for='imageUpload'>
                  <div class="profilePic">
                  
                    {{#if this.picUrl}}
                    <img class="img-rounded" id='profileImage' width="180" height="180" src={{this.picUrl}}>
                    {{/if}}
                    {{#unless this.picUrl}}
                    <img class="img-rounded" id='profileImage' width="180" height="180" src='/assets/images/placeholder.png'>
                    {{/unless}}
                  
                    <div class="overlay">
                      <div class="text"><span class="glyphicon glyphicon-camera"></span> update profile picture</div>
                    </div>
                  </div>
                </label>
                <input type='file' id='imageUpload' name='imageUpload'>
              </form>
            </div>
            <div class="col-xs-7">
              <div class="row">
                <h1 class="display-3">{{this.firstName}} {{this.lastName}}</h1>
              </div>
              <div class="row">
                <div id="tutorRating"></div>
              </div>
              <div class="row">
                <p class="lead">{{this.username}}</p>
              </div>
            </div>
            <div class="row">
              <form action='/logout' method='get'>
                <button id="signOut" class="btn btn-success pull-right">sign out</button>
              </form>
              <div class="countDown">
                <div class="counter" data-count="200">
                  <p>total earnings</p>$200</div>
              </div>
            </div>
          </div>
          <div class="row">
            <!-- <img src="assets/images/edufyIcon.png" width="15%"> -->
            <!--            <button id="signOut" class="btn btn-success pull-right">sign out</button>-->
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- end jumbotron -->

  <div class="container">
    <div class="row">
      <div class="col-sm-3">
        <a href="#" id='lol'>
          <h1 class="text-left" style="padding-bottom: 20px">subjects</h1>
        </a>
        <button class="headers text-left" id='modalSignUp1'>sessions</button>
        <div id="listMonth"></div>
      </div>
      <div class="col-sm-9">
        <div id="calendar" class="calendar-body"></div>
      </div>
    </div>
  </div>

  <!--  Pick classes-->
  <div class="modal fade" id="tutorClasses" role="dialog">
    <div id="tutorClasses" class="modal-dialog modal-md" align="center">
      <div class="modal-content">
        <div class="modal-header">
          <h4 class="modal-title"><img src="assets/images/edufyIcon.png" alt="edufy icon" height="30px" width="30px"> your proficiencies</h4>
        </div>
        <div class="modal-body" align="center">


          <!-- courses -->
          <div class="row">
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="liberalArts">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/liberalArts.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">liberal arts</h2>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="business">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/business.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">business</h2>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="engineering">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/engineering.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">engineering</h2>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="mathematics">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/mathematics.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">mathematics</h2>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="biology">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/biology.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">biology</h2>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="chemistry">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/chemistry.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">chemistry</h2>
                  </div>
                </div>
              </div>
            </div>
          </div>
          <div class="row">
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="computerScience">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/computerScience.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">computer science</h2>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="geology">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/geology.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">geology</h2>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-sm-4">
              <div class="grid-tab classModal" value="physics">
                <div class="hovereffect">
                  <img class="img-responsive" src="assets/images/physics.gif" alt="">
                  <div class="overlayGrid">
                    <h2 id="h2tab">physics</h2>
                  </div>
                </div>
              </div>
            </div>
          </div>


          <!-- form area -->
          <textarea class="form-control" id="specificClass" placeholder="input your classes for this subject" rows="3"></textarea>
          <div class="modal-footer">
            <button id="tutorClassesSubmit" type="button" class="btn btn-success btn-block">submit</button>
          </div>
        </div>
      </div>
    </div>
    </div>
  
  <div class="modal fade" id="freeModal" role="dialog">
      <div class="modal-dialog modal-sm" align="center">
        <div class="modal-content">
          <div class="modal-header">
            <h4 class="modal-title"><img src="assets/images/edufyIcon.png" alt="edufy icon" height="30px" width="30px"> calendar</h4>
          </div>
          <div id="free" class="modal-body">
<!--             <p>are you free between " + displayStart.format('hh:mm T') + "M and " + displayEnd.format('hh:mm T') + "M?"</p> -->
          </div>
          <div class="modal-footer">
            <button id="confirmFree" type="button" class="btn btn-success btn-block" data-dismiss="modal">confirm</button>
          </div>
        </div>
      </div>
    </div>
    
  
    
    
